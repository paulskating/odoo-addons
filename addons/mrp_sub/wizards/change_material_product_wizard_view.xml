<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="change_material_product_wizard_view_form" model="ir.ui.view">
            <field name="name">change_material_product_wizard_view_form</field>
            <field name="model">mrp_sub.change_material_product_wizard</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <h1>Change Product Wizard</h1>
                        <group>
                            <field name="from_bom_product_id"/>
                            <field name="current_product_id"/>
                            <field name="new_product_id"/>
                        </group>
                    </sheet>
                    <footer>
                        <button type="object" name="action_change_product" string="Change Product" class="oe_highlight"/>
                        <button special="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <act_window id="" name="" res_model=""
                    view_mode="form" target="new" multi="False"/>

    </data>
</openerp>